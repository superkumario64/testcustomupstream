{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
          "type": "package",
          "package": {
            "name": "ckeditor/templates",
            "type": "drupal-library",
            "version": "4.11.1",
            "dist": {
              "type": "zip",
              "url": "https://download.ckeditor.com/templates/releases/templates_4.11.1.zip",
              "reference": "master"
            }
          }
        },
        {
            "type": "path",
            "url": "web/modules/custom/curator"
        },
        {
            "type": "path",
            "url": "web/modules/custom/custom_module"
        },
        {
            "type": "path",
            "url": "web/modules/custom/kwall_alert_system"
        },
        {
            "type": "path",
            "url": "web/modules/custom/kwall_map"
        },
        {
            "type": "path",
            "url": "web/modules/custom/kwall_share_bar"
        },
        {
            "type": "path",
            "url": "web/themes/custom/santa_cruz"
        }
    ],
    "require": {
        "bower-asset/jquery-gridder": "^1.4",
        "bower-asset/jquery-simple-color": "^1.2",
        "bower-asset/lightgallery": "^1.8",
        "bower-asset/matchheight": "^0.7.2",
        "bower-asset/owl-carousel2": "^2.3",
        "bower-asset/spectrum": "^1.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/ace_editor": "^1.2",
        "drupal/address": "^1.8",
        "drupal/admin_toolbar": "^2.4",
        "drupal/allowed_formats": "^1.3",
        "drupal/asset_injector": "^2.7",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/backup_migrate": "^4.1",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/block_class": "^1.2",
        "drupal/captcha": "^1.1",
        "drupal/ckeditor_templates": "^1.2",
        "drupal/color_field": "^2.4",
        "drupal/config_partial_export": "^1.6",
        "drupal/config_split": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/core-recommended": "^8.9",
        "drupal/core-vendor-hardening": "^8.9",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^4.0",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/devel_php": "^1.3",
        "drupal/dropzonejs": "^2.3",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/entity": "^1.1",
        "drupal/entity_reference_revisions": "^1.8",
        "drupal/fences": "^2.0@RC",
        "drupal/field_group": "^3.1",
        "drupal/file_mdm": "^2.1",
        "drupal/focal_point": "^1.5",
        "drupal/fontawesome": "^2.17",
        "drupal/fontawesome_menu_icons": "^1.9",
        "drupal/google_analytics": "^3.1",
        "drupal/group": "^1.3",
        "drupal/groupmenu": "^1.0@beta",
        "drupal/gtranslate": "^1.13",
        "drupal/html_title": "^1.0",
        "drupal/imagemagick": "^3.1",
        "drupal/libraries": "^3.0",
        "drupal/link_attributes": "^1.11",
        "drupal/link_class": "^2.0",
        "drupal/linked_field": "^1.3",
        "drupal/linkit": "^6.0",
        "drupal/markup": "^1.0@beta",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_bulk_upload": "^1.0@alpha",
        "drupal/media_entity_file_replace": "^1.0",
        "drupal/menu_block": "1.x-dev",
        "drupal/metatag": "^1.14",
        "drupal/module_filter": "^3.1",
        "drupal/name": "1.x-dev",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/quick_node_clone": "^1.12",
        "drupal/quicklink": "^1.3",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/search_api": "^1.17",
        "drupal/search_api_solr": "^4.1",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/soundcloudfield": "^1.0@alpha",
        "drupal/switch_page_theme": "^1.0",
        "drupal/token": "^1.7",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_link_attributes": "1.x-dev",
        "drupal/twig_tweak": "^2.8",
        "drupal/twig_vardumper": "2.2",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/variationcache": "^1.0",
        "drupal/verf": "^1.0",
        "drupal/views_bulk_operations": "^3.9",
        "drupal/views_templates": "^1.1",
        "drupal/viewsreference": "2.0-beta2",
        "drupal/webform": "^5.22",
        "drupal/wysiwyg_template": "^2.1",
        "drupal/xmlsitemap": "^1.0",
        "drupal/zurb_foundation": "~6.0",
        "drush/drush": "^10.3",
        "grasmash/drupal-security-warning": "^1.1",
        "npm-asset/dropzone": "^5.7",
        "npm-asset/fontawesome-iconpicker": "^3.2",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^1.1",
        "topfloor/composer-cleanup-vcs-dirs": "^1.0",
        "vlucas/phpdotenv": "^2.4",
        "wikimedia/composer-merge-plugin": "^1.4",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8",
        "sort-packages": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-types": [
            "component",
            "bower-asset",
            "ckeditor-plugin",
            "npm-asset"
        ],
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:component",
                "type:npm-asset"
            ],
            "web/libraries/ckeditor/plugins/{$name}": ["type:ckeditor-plugin"],
            "web/libraries/chosen": ["npm-asset/chosen-js"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/*/composer.libraries.json",
                "web/modules/custom/*/composer.json",
                "web/themes/custom/*/composer.json"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": {
                    "mode": "skip"
                },
                "[web-root]/robots.txt": {
                    "mode": "skip"
                },
                "[project-root]/.editorconfig": false
            }
        },
        "patches": {
            "drupal/core": {
                "Incorrect filter group OR behavior, LEFT JOIN changed to INNER JOIN": "https://www.drupal.org/files/issues/2019-10-08/1766338-106.patch",
                "Migrations fail due to missing dependency when dependency is clearly not missing.": "https://www.drupal.org/files/issues/2019-02-18/2797505-13.patch",
                "Incorrect parent tree shows when the same URL is used in menu links many times": "https://www.drupal.org/files/issues/2859703-find-correct-link-6.patch",
                "Node revision routes don't convert {node} param": "https://www.drupal.org/files/issues/2019-04-04/2730631_85_0.patch",
                "There is no way to delete or update file entities of other users incl. user/1": "https://www.drupal.org/files/issues/2020-01-13/2949017-62.patch",
                "Allow media items to be edited in a modal when using the field widget": "https://www.drupal.org/files/issues/2020-06-06/2985168-43.patch",
                "Content Edit page jumps when media library item is placed": "https://www.drupal.org/files/issues/2019-11-04/3073023-4.patch",
                "Default theme favicon for administration theme": "https://www.drupal.org/files/issues/2019-12-14/default-theme-favicon-for-administration-theme-2981434-7.patch",
                "[D8] Chrome 83 cancels jquery.form ajax requests over https": "https://www.drupal.org/files/issues/2020-05-27/3138421-75.patch",
                "Add Views EntityReference filter to be available for all entity reference fields": "https://www.drupal.org/files/issues/2020-05-23/drupal-generalize-taxonomyindextid-filter-2429699-325.patch",
                "[regression] Restore styling for embedded media edit button in Seven theme": "https://www.drupal.org/files/issues/2020-02-25/3092795-22.patch",
                "Constrain the width of aligned images, media, blockquotes etc to a max of 75%": "https://www.drupal.org/files/issues/2019-10-10/3078287-104.patch",
                "views row display":  "web/patches/2552541-30.patch"
            },
            "drupal/address": {
                "The locality.name data is missed in PlainFormater": "https://www.drupal.org/files/issues/locality-name-data-missed-plainformater-2881391-1.patch"
            },
            "drupal/group": {
                "Reapply patch from https://www.drupal.org/files/issues/2797793-20.patch manually without db update.": "patches/group-groupmenu-patch-manual-rc5-reroll.patch",
                "Custom patch to address Group errors from menus that were created without being assigned to Groups.": "patches/group-fatal-errors.patch",
                "Add support for Bulk Content to Group": "https://www.drupal.org/files/issues/2019-12-13/group_add-support-for-vbo-2865642-22.patch"
            },
            "drupal/groupmenu": {
                "UndefinedLinkTemplateException": "https://www.drupal.org/files/issues/2880714.patch",
                "Call to a member function id() on null in /modules/contrib/groupmenu/src/GroupMenuConfigOverrides.php on line 214": "patches/groupmenu-config-override-null-issue.patch",
                "Custom patch to address Group Menu errors from menus that were created without being assigned to Groups.": "patches/groupmenu-fatal-errors.patch",
                "Group Menu Block: Active Trail": "patches/groupmenu_activetrail.patch"
            },
            "drupal/menu_block": {
                "Hide menu block on pages not included in menu": "https://www.drupal.org/files/issues/2020-08-24/menu_block-hide_on_inactive-3007225-8.patch",
                "Ability show items on same level if active menu level don't have children": "https://www.drupal.org/files/issues/2020-06-26/menu_block-active-level-fallback-when-no-children-758184-31-D8.patch",
                "Remove duplicate \"expand all menu items\" checkbox introduced by core in Drupal 8.7": "https://www.drupal.org/files/issues/2020-06-24/3133567-use-core-expand-all-items-checkbox-16.patch"
            },
            "drupal/masquerade": {
                "All users, also from not allowed user roles, showing up in auto complete": "https://www.drupal.org/files/issues/2019-06-16/masquerade_filter-roles_3003796_5.patch"
            },
            "drupal/paragraphs" : {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null" : "https://www.drupal.org/files/issues/2019-08-10/paragraphs-set_langcode_widgets-290139_updated.patch"
            },
            "drupal/soundcloudfield" : {
                "Embed stopped working, update oembed URL to https://": "https://www.drupal.org/files/issues/2019-08-15/soundcloud-oembed_https-3075203-2-D8.patch",
                "Avoid using curl, use the drupal client instead": "https://www.drupal.org/files/issues/2020-02-10/3111931-4.patch",
                "Deprecated static call to Drupal\\soundcloudfield\\Plugin\\Field\\FieldWidget\\SoundCloudWidget::validateSoundCloudUriElement()": "https://www.drupal.org/files/issues/2020-04-06/3086396-soundcloudfield-fix_for_deprecated_static_call.patch"
            },
            "drupal/fontawesome": {
                "Allow to use Brand icons in CKeditor": "https://www.drupal.org/files/issues/2020-05-26/fontawesome-allow-to-use-brand-icons-in-cKeditor-3142962-2.patch"
            },
            "drupal/tb_megamenu": {
                "Missing array keys cause notices": "https://www.drupal.org/files/issues/missing_array_keys-2921195-3.patch"
            },
            "drupal/multiline_config": {
                "Multiline doesn't work with Config Split": "https://www.drupal.org/files/issues/2020-01-21/3056560-2.patch"
            },
            "drupal/viewsreference": {
                "Is there any way to pass the token from the current paragraph to the view?": "https://www.drupal.org/files/issues/2020-08-17/viewsreference-argument-tokens-2985364-13.patch"
            }
        }
    }
}
